class sample{
    sample(int n){
        if(n%2==1){
            System.out.println("ODD");
        }else{
            System.out.println("EVEN");
        }
    }
    public static void main(String args[]){
        System.out.println("Arigato");
        sample obj=new sample(2347);
        sample obj1=new sample(7432);
        System.out.println("THANK YOU");
    }
}
